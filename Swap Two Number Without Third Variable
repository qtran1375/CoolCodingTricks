public class Test
{
	public static void main (String[]args)
	{
		int x = 20;
		int y = 30;


				System.out.println("x: " + x);
				System.out.println("y: " + y);


		x -=- y;
		y = x - y;
		x -= y;

				System.out.println("x: " + x);
				System.out.println("y: " + y);



	}
}


